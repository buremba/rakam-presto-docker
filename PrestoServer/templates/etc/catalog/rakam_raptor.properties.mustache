connector.name=rakam_raptor

backup.timeout=20m

backup.provider=s3
aws.s3-bucket={{S3_BUCKET}}
aws.region={{AWS_REGION}}

metadata.db.type=mysql
metadata.db.url=jdbc:mysql://{{DB_ADDRESS}}/{{DB_NAME}}?user={{DB_USERNAME}}&password={{DB_PASSWORD}}
metadata.db.connections.max=200

storage.data-directory=var/data

cache.data-size=2GB
cache.method=memory_mapped
